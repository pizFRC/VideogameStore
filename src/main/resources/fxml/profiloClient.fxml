<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.ProfiloClientController">
   <center>
      <VBox alignment="CENTER" prefHeight="600.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Separator prefHeight="26.0" prefWidth="415.0" style="-fx-background-color: #c05442;" />
            <HBox alignment="CENTER" minHeight="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#changeImg" text="Button" HBox.hgrow="ALWAYS">
                     <graphic>
                        <ImageView fx:id="imgProfilo" fitHeight="200.0" fitWidth="250.0" pickOnBounds="true" preserveRatio="true" />
                     </graphic>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" minHeight="100.0" prefHeight="100.0" style="-fx-background-color: #c05442;" VBox.vgrow="ALWAYS">
               <children>
                  <Label text="Username">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin></Label>
                  <Separator opacity="0.0" orientation="VERTICAL" prefHeight="200.0" prefWidth="20.0" />
                  <TextField fx:id="usernameField" alignment="CENTER" minHeight="50.0" minWidth="300.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin></TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER" minHeight="100.0" prefHeight="100.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label text="Password">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin></Label>
                  <Separator opacity="0.0" orientation="VERTICAL" prefHeight="200.0" prefWidth="20.0" />
                  <TextField fx:id="pwField" alignment="CENTER" minHeight="50.0" minWidth="300.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin></TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER" minHeight="100.0" prefHeight="100.0" style="-fx-background-color: #c05442;" VBox.vgrow="ALWAYS">
               <children>
                  <Label text="Email">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin></Label>
                  <Separator opacity="0.0" orientation="VERTICAL" prefHeight="200.0" prefWidth="45.0" />
                  <TextField fx:id="emailField" alignment="CENTER" minHeight="50.0" minWidth="300.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin></TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER" minHeight="100.0" prefHeight="100.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label text="Phone">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin></Label>
                  <Separator opacity="0.0" orientation="VERTICAL" prefHeight="200.0" prefWidth="45.0" />
                  <TextField fx:id="phoneField" alignment="CENTER" minHeight="50.0" minWidth="300.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin></TextField>
               </children>
            </HBox>
            <Separator prefHeight="26.0" prefWidth="415.0" style="-fx-background-color: #c05442;" />
         </children>
         <BorderPane.margin>
            <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
